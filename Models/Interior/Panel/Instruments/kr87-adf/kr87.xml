    <inherits-from>../../../../Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <name>in-use-leds</name>
    <object-name>freq.001</object-name>
    <object-name>freq.002</object-name>
    <object-name>freq.003</object-name>
    <object-name>freq.004</object-name>
  </animation>

  <animation>
    <name>standby-leds</name>
    <object-name>stby.001</object-name>
    <object-name>stby.002</object-name>
    <object-name>stby.003</object-name>
    <object-name>stby.004</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>KR87.adf</object-name>
    <object-name>KR87.bfo</object-name>
    <object-name>KR87.frq</object-name>
    <object-name>KR87.flt</object-name>
    <object-name>KR87.set</object-name>
    <object-name>KR87.volume</object-name>
    <object-name>KR87.volume</object-name>
    <object-name>KR87.knob</object-name>
    <object-name>KR87.knob1</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/luminosite-lights</factor-prop>
      <red>   0.2 </red>
      <green> 0.1 </green>
      <blue>  0.1 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>in-use-leds</object-name>
    <object-name>standby-leds</object-name>
    <object-name>ant.annun</object-name>
    <object-name>adf.annun</object-name>
    <object-name>bfo.annun</object-name>
    <object-name>frq.annun</object-name>
    <object-name>flt.annun</object-name>
    <object-name>et.annun</object-name>
    <object-name>stby</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/luminosite-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.40 </green>
      <blue>  0.30 </blue>
    </emission>
  </animation>

  <animation>
    <type>select</type>
    <object-name>in-use-leds</object-name>
    <object-name>standby-leds</object-name>
    <object-name>ant.annun</object-name>
    <object-name>adf.annun</object-name>
    <object-name>bfo.annun</object-name>
    <object-name>frq.annun</object-name>
    <object-name>flt.annun</object-name>
    <object-name>et.annun</object-name>
    <object-name>stby</object-name>
    <condition>
      <and>
        <property>instrumentation/adf[0]/power-btn</property>
        <greater-than>
          <property>systems/electrical/outputs/adf</property>
          <value>18</value>
        </greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>freq.001</object-name>
    <condition>
      <greater-than>
        <property>instrumentation/adf[0]/frequencies/selected-khz</property>
        <value>999</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>stby.001</object-name>
    <condition>
      <greater-than>
        <property>instrumentation/adf[0]/right-display</property>
        <value>999</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>stby.002</object-name>
    <condition>
      <greater-than>
        <property>instrumentation/adf[0]/right-display</property>
        <value>99</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>stby</object-name>
    <condition>
      <property>instrumentation/adf[0]/display-mode</property>
    </condition>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>KR87.volume</object-name>
    <property>instrumentation/adf[0]/volume-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>   0 </dep></entry>
      <entry><ind> 1 </ind><dep> 300 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.0000 </x-m>
      <y-m>  0.0380 </y-m>
      <z-m> -0.0062 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>KR87.adf</object-name>
    <property>instrumentation/adf[0]/adf-btn</property>
    <factor> 0.001 </factor>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>KR87.bfo</object-name>
    <property>instrumentation/adf[0]/bfo-btn</property>
    <factor> 0.001 </factor>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- FREQUENCIES -->
  <animation>
    <type>textranslate</type>
    <object-name>freq.001</object-name>
    <property>instrumentation/adf[0]/frequencies/selected-khz</property>
    <factor> 0.0001 </factor>
    <step> 1000 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>freq.002</object-name>
    <property>instrumentation/adf[0]/frequencies/selected-khz</property>
    <factor> 0.001 </factor>
    <step> 100 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>freq.003</object-name>
    <property>instrumentation/adf[0]/frequencies/selected-khz</property>
    <factor> 0.01 </factor>
    <step> 10 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>freq.004</object-name>
    <property>instrumentation/adf[0]/frequencies/selected-khz</property>
    <factor> 0.1 </factor>
    <step> 1 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Standby -->
  <animation>
    <type>textranslate</type>
    <object-name>stby.001</object-name>
    <property>instrumentation/adf[0]/right-display</property>
    <factor> 0.0001 </factor>
    <step> 1000 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>stby.002</object-name>
    <property>instrumentation/adf[0]/right-display</property>
    <factor> 0.001 </factor>
    <step> 100 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>stby.003</object-name>
    <property>instrumentation/adf[0]/right-display</property>
    <factor> 0.01 </factor>
    <step> 10 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>stby.004</object-name>
    <property>instrumentation/adf[0]/right-display</property>
    <factor> 0.1 </factor>
    <step> 1 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>ant.annun</object-name>
    <condition>
      <not><property>instrumentation/adf[0]/adf-btn</property></not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>adf.annun</object-name>
    <condition>
      <property>instrumentation/adf[0]/adf-btn</property>
      <not><property>instrumentation/adf[0]/bfo-btn</property></not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>bfo.annun</object-name>
    <condition>
      <property>instrumentation/adf[0]/adf-btn</property>
      <property>instrumentation/adf[0]/bfo-btn</property>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>et.annun</object-name>
    <condition>
      <equals>
        <property>instrumentation/adf[0]/display-mode</property>
        <value>2</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>flt.annun</object-name>
    <condition>
      <equals>
        <property>instrumentation/adf[0]/display-mode</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>frq.annun</object-name>
    <condition>
      <equals>
        <property>instrumentation/adf[0]/display-mode</property>
        <value>0</value>
      </equals>
    </condition>
  </animation>

  <!-- OSG Hotspots -->
  <animation>
    <type>pick</type>
    <object-name>KR87.knob1</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/frequencies/standby-khz</property>
        <step>1</step>
        <min>100</min>
        <max>1300</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.knob1</object-name>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/frequencies/standby-khz</property>
        <step>-1</step>
        <min>100</min>
        <max>1300</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.knob1</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/frequencies/standby-khz</property>
        <step>1</step>
        <min>100</min>
        <max>1300</max>
        <wrap>true</wrap>
