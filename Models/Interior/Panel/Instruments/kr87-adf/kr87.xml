        <max>1300</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.knob</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/frequencies/standby-khz</property>
        <step>100</step>
        <min>100</min>
        <max>1300</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.knob</object-name>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/frequencies/standby-khz</property>
        <step>-100</step>
        <min>100</min>
        <max>1300</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.knob</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/frequencies/standby-khz</property>
        <step>100</step>
        <min>100</min>
        <max>1300</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.knob</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/frequencies/standby-khz</property>
        <step>-100</step>
        <min>100</min>
        <max>1300</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.volume</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <condition>
          <property>instrumentation/adf[0]/power-btn</property>
        </condition>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/volume-norm</property>
        <step>0.05</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <condition>
          <not>
            <property>instrumentation/adf[0]/power-btn</property>
          </not>
        </condition>
        <command>property-assign</command>
        <property>instrumentation/adf[0]/power-btn</property>
        <value>1</value>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.volume</object-name>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <condition>
          <greater-than>
            <property>instrumentation/adf[0]/volume-norm</property>
            <value>0.0</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/volume-norm</property>
        <step>-0.05</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <condition>
          <equals>
            <property>instrumentation/adf[0]/volume-norm</property>
            <value>0</value>
          </equals>
        </condition>
        <command>property-assign</command>
        <property>instrumentation/adf[0]/power-btn</property>
        <value>0</value>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.volume</object-name>
    <action>
      <button>3</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <condition>
          <property>instrumentation/adf[0]/power-btn</property>
        </condition>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/volume-norm</property>
        <step>0.05</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <condition>
          <not>
            <property>instrumentation/adf[0]/power-btn</property>
          </not>
        </condition>
        <command>property-assign</command>
        <property>instrumentation/adf[0]/power-btn</property>
        <value>1</value>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.volume</object-name>
    <action>
      <button>4</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <condition>
          <greater-than>
            <property>instrumentation/adf[0]/volume-norm</property>
            <value>0.0</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>instrumentation/adf[0]/volume-norm</property>
        <step>-0.05</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <condition>
          <equals>
            <property>instrumentation/adf[0]/volume-norm</property>
            <value>0</value>
          </equals>
        </condition>
        <command>property-assign</command>
        <property>instrumentation/adf[0]/power-btn</property>
        <value>0</value>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.adf</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>instrumentation/adf[0]/adf-btn</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.bfo</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>instrumentation/adf[0]/bfo-btn</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.frq</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <condition>
          <equals>
            <property>instrumentation/adf[0]/display-mode</property>
            <value>0</value>
          </equals>
        </condition>
        <command>property-swap</command>
        <property>instrumentation/adf[0]/frequencies/selected-khz</property>
        <property>instrumentation/adf[0]/frequencies/standby-khz</property>
      </binding>
    </action>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <condition>
          <greater-than>
            <property>instrumentation/adf[0]/display-mode</property>
            <value>0</value>
          </greater-than>
        </condition>
        <command>property-assign</command>
        <property>instrumentation/adf[0]/display-mode</property>
        <value>0</value>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KR87.flt</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <condition>
          <not-equals>
            <property>instrumentation/adf[0]/display-mode</property>
            <value>1</value>
          </not-equals>
        </condition>
        <command>property-assign</command>
        <property>instrumentation/adf[0]/display-mode</property>
        <value>3</value>
      </binding>
      <binding>
        <condition>
          <equals>
            <property>instrumentation/adf[0]/display-mode</property>
            <value>1</value>
          </equals>
        </condition>
        <command>property-assign</command>
        <property>instrumentation/adf[0]/display-mode</property>
        <value>4</value>
      </binding>
      <binding>
        <condition>
          <equals>
            <property>instrumentation/adf[0]/display-mode</property>
            <value>3</value>
          </equals>
        </condition>
