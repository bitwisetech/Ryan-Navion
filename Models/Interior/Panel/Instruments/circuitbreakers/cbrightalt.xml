<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>cb_70.ac</path>

  <animation>
    <object-name>support</object-name>
    <object-name>breaker</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>support</object-name>
    <object-name>breaker</object-name>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>breaker</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/electric/circuitbreaker/rightalt</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>breaker</object-name>
    <property>/controls/electric/circuitbreaker/rightalt</property>
    <factor>0.007</factor>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
